#!/bin/bash
rm -f ids.json && echo [ >> ids.json && for ((i = 1; i <= $1; i++)); do  \
  curl --header "Content-Type: application/json" \
  --request POST \
  --data '{"user":"yo","pwd":"kk"}' \
  http://localhost:7512/nyc-open-data/yellow-taxi/_create >> ids.json && \
      if [ $i != $1 ];
	 then
	     echo , >> ids.json;
	     fi;
       done
      echo ] >> ids.json && python3 retrieve_id.py

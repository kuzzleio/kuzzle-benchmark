#!/bin/bash
echo [ >> test.json && for ((i = 1; i <= $1; i++)); do  \
  curl --header "Content-Type: application/json" \
  --request POST \
  --data '{"user":"yo","pwd":"kk"}' \
  http://localhost:7512/nyc-open-data/yellow-taxi/_create >> test.json && \
      if [ $i != $1 ];
	 then
	     echo , >> test.json;
	     fi;
       done
      echo ] >> test.json\
